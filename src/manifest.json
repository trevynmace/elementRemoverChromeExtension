{
  "manifest_version": 2,
  "name": "Element Remover",
  "version": "0.2",
  "background": {
  	"scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "https://kanbanflow.com/board/*"
      ],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
  	"default_icon": "icon.png"
  }
}